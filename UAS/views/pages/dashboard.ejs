    }
    #NavContent {
        margin-right: 2%;

        border: 2px solid gray;
    }

    /* KATALOG */
    #list-catalog .card {
        /* position: inherit; */
        overflow: hidden;
        margin: 0 2%;
        text-decoration: none;
        color: black;
        display: flex;
        height: 275px;
        align-items: stretch;
        border: 1px solid gray;
    }

    #list-catalog .card-img-top {
        height: 100%;
        padding: 10%;
    }

    #list-catalog .card-body {
        border-top: 1px solid gray;
        background-color: rgb(200, 200, 200);
    }

    #list-catalog .card:hover .card-body {
        color: white;
        background-color: rgb(150, 150, 150);
        transition: 0.2s;
    }

    /* ORDER */
    #list-order .card {
        margin: 1% 2%;
        border: 1px solid gray;
        padding: 1% 2%;
        text-decoration: none;
        color: black;
        height: fit-content;
        align-items: center;
        flex-direction: row;
    }

    #list-order .card-desc {
        margin-left: 2%;
        width: 40%;
        line-height: 40%;
        margin-top: 1%;
    }

    #list-order .card-desc span {
        float: right;
    }
    #list-order .card-desc .total {
        border-top: 1px solid gray;
        padding-top: 3%;
    }

    #list-order .card-img-left {
        width: 10%;
    }

    #list-order .card:hover {
        background-color: rgb(230, 230, 230);
        transition: 0.2s;
    }

    /* MESSAGES */
    #list-messages .card {
        margin: 1% 2%;
        border: 1px solid gray;
        padding: 1% 2%;
        text-decoration: none;
        color: black;
        height: fit-content;
        align-items: center;
        flex-direction: row;
    }
    #list-messages .card-avatar {
        width: 20%;
        margin-right: 2%;
        border-radius: 100%;
        border: 1px solid gray;
    }
    #list-messages .card-desc {
        flex-grow: initial;
    }

    #list-messages .card-desc p {
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
        overflow: hidden;
        text-overflow: ellipsis;
    }
    #list-messages .card:hover {
        background-color: rgb(230, 230, 230);
        transition: 0.2s;
    }

    #chart {
        border: 1px solid black;
        margin: 0 auto;
        width: 96%;
        border-radius: 10px;
        padding: 2% 10%;
        height: 400px;
    }
    #chart tbody {
        margin: 2% 0;
    }
    #list-add-product div {
        align-items: flex-start;
        padding: 2% 0;
        border-bottom: 1px solid rgb(200, 200, 200);
    }
    /* media query */
    @media screen and (max-width: 770px) {
        #chart {
            padding: 2% 6%;
        }
        #chart tbody {
            height: 320px;
            font-size: 12px;
        }
        #list-order .card-desc {
            font-size: 13px;
            width: 60%;
        }
        #list-order .card-status {
            font-size: 13px;
        }
        #list-order .card-status h4 {
            font-size: 18px;
        }
        .card-group-messages {
            font-size: smaller;
        }
        #list-messages .card-desc {
            width: 100%;
        }
    }
</style>

